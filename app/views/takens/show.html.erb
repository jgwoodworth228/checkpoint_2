<div class="page-header">
  <h3>Taken #<%= @taken.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @taken.user_id %></dd>

      <dt>Course</dt>
      <dd><%= @taken.course_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/takens" class="btn btn-primary">
        Back
      </a>
      <a href="/takens/<%= @taken.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_taken/<%= @taken.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
